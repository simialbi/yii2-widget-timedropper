{
    "name": "simialbi/yii2-widget-timedropper",
    "type": "yii2-extension",
    "description": "Yii 2 Widget for timedropper jquery plugin of felice gattuso",
    "keywords": [
        "yii2",
        "timedropper",
        "timepicker",
        "picker"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "simialbi",
            "email": "simi.albi@outlook.com"
        },
        {
            "name": "Felice Gattuso",
            "email": "me@felicegattuso.com"
        }
    ],
    "support": {
        "source": "https://github.com/simialbi/yii2-widget-timedropper",
        "issues": "https://github.com/simialbi/yii2-widget-timedropper/issues"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "yiisoft/yii2": "^2.0.20",
        "simialbi/yii2-simialbi-base": ">=0.13.1 <1.0 | ^1.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-coding-standards": "~2.0",
        "yiisoft/yii2-bootstrap4": "^2.0.4"
    },
    "autoload": {
        "psr-4": {
            "simialbi\\yii2\\timedropper\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
